		JFrame myFrame = new JFrame("Суперокно");
		myFrame.setSize(600, 100);
		myFrame.setDefaultCloseOperation(WindowConstants.DO_NOTHING_ON_CLOSE);
		myFrame.setVisible(true);
	}

}